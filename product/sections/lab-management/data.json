  "pitches": [
    {
      "id": "pitch-001",
      "title": "Système de réservation des formations",
      "status": "building",
      "appetite": "6-weeks",
      "authorId": "member-001",
      "createdAt": "2025-02-15",
      "problem": "Les inscriptions aux formations se font par email, ce qui génère beaucoup d'échanges manuels et des erreurs (doubles inscriptions, paiements non suivis). Les formateurs passent 2-3h par formation juste pour gérer l'administratif.",
      "solution": "Un système de réservation en ligne intégré au site avec confirmation automatique, liste d'attente, et suivi des paiements. Le formateur reçoit une liste consolidée la veille de la formation.",
      "rabbitHoles": [
        "Tentation d'ajouter un système de paiement complet dès le début",
        "Intégration calendrier externe (Google, Outlook) - à éviter pour V1",
        "Gestion des remboursements - garder manuel pour l'instant"
      ],
      "noGos": [
        "Pas de paiement en ligne dans cette version",
        "Pas de récurrence automatique des formations",
        "Pas d'intégration avec des outils tiers"
      ],
      "breadboard": {
        "places": ["Catalogue formations", "Fiche formation", "Formulaire inscription", "Confirmation", "Dashboard formateur"],
        "affordances": ["Voir détails", "S'inscrire", "Confirmer présence", "Annuler", "Exporter liste"],
        "connections": [
          { "from": "Catalogue formations", "to": "Fiche formation", "via": "Voir détails" },
          { "from": "Fiche formation", "to": "Formulaire inscription", "via": "S'inscrire" },
          { "from": "Formulaire inscription", "to": "Confirmation", "via": "Valider" }
        ]
      },
      "fatMarkerSketch": "data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iNDAwIiBoZWlnaHQ9IjMwMCI+PC9zdmc+"
    },
    {
      "id": "pitch-002",
      "title": "Dashboard de suivi des projets clients",
      "status": "building",
      "appetite": "6-weeks",
      "authorId": "member-002",
      "createdAt": "2025-02-18",
      "problem": "Les clients n'ont aucune visibilité sur l'avancement de leur projet de jardin-forêt. Ils appellent régulièrement pour demander où on en est, ce qui interrompt le travail des designers.",
      "solution": "Une page client simple et visuelle montrant les grandes étapes du projet (design, validation, plantation, suivi) avec le statut actuel et les prochaines actions attendues.",
      "rabbitHoles": [
        "Vouloir montrer trop de détails techniques",
        "Système de commentaires/chat avec le client",
        "Notifications push"
      ],
      "noGos": [
        "Pas d'édition par le client",
        "Pas de partage de documents sensibles (devis détaillés)",
        "Pas de système de messagerie intégré"
      ],
      "breadboard": {
        "places": ["Page projet client", "Timeline", "Documents partagés"],
        "affordances": ["Voir étape", "Télécharger doc", "Contacter équipe"],
        "connections": [
          { "from": "Page projet client", "to": "Timeline", "via": "Voir avancement" },
          { "from": "Page projet client", "to": "Documents partagés", "via": "Voir documents" }
        ]
      },
      "fatMarkerSketch": null
    },
    {
      "id": "pitch-003",
      "title": "Outil de sélection végétale",
      "status": "shaped",
      "appetite": "6-weeks",
      "authorId": "member-001",
      "createdAt": "2025-03-20",
      "problem": "La sélection des espèces pour un projet est fastidieuse : on consulte plusieurs sources, on croise manuellement les critères (sol, exposition, usages), et on oublie souvent des espèces pertinentes.",
      "solution": "Un outil de filtrage multi-critères sur la base de données végétale, avec sauvegarde de sélections et export vers les documents projet.",
      "rabbitHoles": [
        "Interface de filtrage trop complexe avec 20+ critères",
        "Suggestions automatiques basées sur l'IA",
        "Import de plans de plantation existants"
      ],
      "noGos": [
        "Pas de création d'espèces depuis cet outil",
        "Pas de visualisation cartographique",
        "Pas de calcul automatique de densités"
      ],
      "breadboard": {
        "places": ["Recherche", "Résultats", "Fiche espèce", "Ma sélection"],
        "affordances": ["Filtrer", "Ajouter à sélection", "Retirer", "Exporter"],
        "connections": []
      },
      "fatMarkerSketch": null
    },
    {
      "id": "pitch-004",
      "title": "Refonte du système de timesheets",
      "status": "raw",
      "appetite": "3-weeks",
      "authorId": "member-003",
      "createdAt": "2025-04-01",
      "problem": "Le système actuel de timesheets est un tableur partagé difficile à maintenir. Les membres oublient de le remplir, les catégories sont incohérentes, et le reporting mensuel prend une journée entière.",
      "solution": "Un formulaire simple intégré à l'app avec rappels automatiques et catégories prédéfinies. Export automatique pour la comptabilité.",
